<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

<appSettings>
	
	<!-- specify the path to a local directory for audio testing. -->
	<add key="NAudioTestingDirPath" value="C:\NAudioTesting\"/>

	<!-- specify the names of a few music files (in .mp3 format). -->
    <add key="LongAudioFile" value="Perturbator - I Am The Night [Full Album].mp3"/>
    <add key="MediumAudioFile" value="PERTURBATOR - Dangerous Days.mp3"/>
	
	<!-- specify the name of a video file, which will be used to demonstrate how to extract audio. -->
    <add key="VideoFile" value="From Sand to Silicon The Making of a Microchip  Intel.mp4"/>
	
	<!-- specify the names of a few voice recordings (in .wav format). -->
    <add key="VoiceQuote_1" value="Nirvana.wav"/>
    <add key="VoiceQuote_2" value="Perfection.wav"/>
    <add key="VoiceQuote_3" value="TheArchitect.wav"/>
    <add key="VoiceQuote_4" value="SpeechTest_8Bit.wav"/>

</appSettings>

</configuration>